{   
    "unibeautify.defaultConfig": "C:/django/.unibeautifyrc.json",
    "emmet.includeLanguages": {
        "django-html": "html",
    },
    "[django-html]": {
        "editor.tabSize": 4,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "Glavin001.unibeautify-vscode"
        },
    
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "entity.tag.tagbraces.django" // {%%} {{}} 全て 
                ],
                "settings": {
                    "foreground": "#fcfcfa"
                }
            },
            {
                "scope": [
                    //"storage.type.templatetag.django", // href={% %}
                    //"storage.type.customtemplatetag.django", // src={% %}
                    //"storage.type.variable.django", // {{ }}
                    "constant.other", // static
                    "entity.tag.filter-pipe.django", // |
                    "entity.name.function.filter.django", // 未確認
                    "comment.line.number-sign.django", // 未確認
                    "constant.other.tag.name.django", // statc等
                    "keyword.control.tag-name.django", // if else url
                    "keyword.operator.filter-argument.django", //オペレーター
                    "keyword.operator.assignment.django", // ==
                    "keyword.operator.argument-separator.django", // ,
                    "keyword.operator.condition.django", // > <
                    "keyword.operator.getter.django", // .
                    "keyword.operator.django", // in and
                    "string.unquoted.tag-string.django" // 文字
                ],
                "settings": {
                    "foreground": "#ab9df2"
                }
            },
        ]
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "editor.fontLigatures": null,
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.folders.color": "#42a5f5",
    "material-icon-theme.activeIconPack": "angular",
}